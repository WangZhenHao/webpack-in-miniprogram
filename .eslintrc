{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "globals": {
      "getApp": false,
      "getCurrentPages": false,
      "Page": false,
      "Component": false,
      "Behavior": false,
      "App": false,
      "wx": false,
      "PRODUCTION": false,
      "__wxConfig": true
  },
  "rules": {
      "no-extend-native": 0,
      "no-shadow": 0,
      "no-loop-func": 0,
      "no-console": 0,
      "no-var": 0,
      "vars-on-top": 0,
      "new-cap": 0,
      "no-used-var": 0,
      "semi": [
          1,
          "always",
          {
              "omitLastInOneLineBlock": true
          }
      ],
      "camelcase": 1,
      "prefer-template": 0,
      "eqeqeq": 0,
      "max-len": 0,
      "react/prefer-stateless-function": 0,
      "comma-dangle": 0,
      "func-names": 0,
      "prefer-const": 0,
      "arrow-body-style": 0,
      "no-param-reassign": 0,
      "no-return-assign": 0,
      "consistent-return": 0,
      "no-unused-expressions": 0,
      "no-underscore-dangle": 0,
      "no-use-before-define": 0,
      "prefer-destructuring": 0,
      "no-mixed-operators": 0,
      "no-new": 0,
      "no-debugger": 0,
      "no-multi-assign": 0,
      "class-methods-use-this": 0,
      "no-plusplus": 0,
      "no-restricted-globals": 0,
      "no-restricted-syntax": 0,
      "import/no-unresolved": [0, { "ignore": "utils\/.+" }],
      "import/extensions": [0],
      "import/prefer-default-export": 0,
      "arrow-parens": 0,
      "prefer-promise-reject-errors": 0
  },
  "settings": {
      "import/resolver": {
          "webpack": {
              "config": "./build/webpack.config.base.js"
          }
      }
  }
}
